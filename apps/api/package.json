    "@babel/traverse": "7.28.4",
    "@babel/types": "7.28.4",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/static": "^8.2.0",
    "@google-cloud/storage": "^7.7.0",
    "@loopyway/entitlements": "workspace:*",
    "@swc/helpers": "0.5.17",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.19",
