{
  "name": "@loopyway/api",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "main": "dist/server.mjs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:worker": "tsx watch src/localdev/worker.run.ts",
    "build": "tsup --config tsup.config.ts",
    "build:bundle": "tsup --config tsup.config.ts",
    "start": "node dist/server.mjs",
    "migrate": "tsx src/migrate.ts",
    "check:bundle": "node -e \"import('file://'+process.cwd()+'/dist/server.mjs').then(()=>console.log('OK')).catch(e=>{console.error(e);process.exit(1)})\""
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.585.0",
