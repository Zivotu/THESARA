    "postcss": "^8.4.33",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stripe": "^14.25.0",
    "tailwindcss": "^3.4.0",
    "tar": "^7.4.3",
    "zod": "^3.25.76"
